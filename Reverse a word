import java.util.*;
public class reverseword {

    public static void main(String[] args){
            String s1;
            System.out.println("enter a word:");
            Scanner sc=new Scanner(System.in);
            s1= sc.nextLine();
            for(int i=s1.length()-1 ; i>=0; i--)
            {
                System.out.println(s1.charAt(i));
            }
    }
}
